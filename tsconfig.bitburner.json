{
  "include": ["NetscriptDefinitions.d.ts", "bitburner/**/*"],
  "exclude": [],
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "bitburner/",
    "outDir": "dist/",
    "baseUrl": "bitburner/",
    "allowJs": true,
    "jsx": "react",
    "types": ["react", "react-dom"],
    "paths": {
      "/*": ["*"],
      "@ns": ["../NetscriptDefinitions.d.ts"],
      "@react": ["lib/react.ts"]
    }
  }
}
